package com.thinhnv.ungdungquanlytailieu.thuvien.tailieu;

public class TapChi extends TaiLieu {
	private int soPhatHanh;
	private short thangPhatHanh;

	public TapChi(String maTL, String tenTL, int soBanPhatHanh, int soPhatHanh, short thangPhatHanh) {
		super(maTL, tenTL, soBanPhatHanh);
		this.soPhatHanh = soPhatHanh;
		this.thangPhatHanh = thangPhatHanh;
	}

}
